{
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  }
}
