{
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/zipcodes/lookup/:zipcode",
      "destination": "/api/zipcode-lookup?zipcode=:zipcode"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
